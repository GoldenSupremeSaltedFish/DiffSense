{
  "name": "diffsense",
  "displayName": "DiffSense - Git Code Impact Analysis",
  "description": "智能Git代码影响分析工具，支持Java、Golang、前端代码的变更影响分析和可视化",
  "version": "0.1.9",
  "publisher": "humphreyLi",
  "author": {
    "name": "DiffSense Team"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoldenSupremeSaltedFish/DiffSense.git"
  },
  "bugs": {
    "url": "https://github.com/GoldenSupremeSaltedFish/DiffSense/issues"
  },
  "homepage": "https://github.com/GoldenSupremeSaltedFish/DiffSense#readme",
  "keywords": [
    "git",
    "code analysis",
    "impact analysis",
    "java",
    "golang",
    "frontend",
    "diff",
    "code review"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Testing",
    "Visualization"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "contributes": {
    "commands": [
      {
        "command": "diffsense.runAnalysis",
        "title": "Run Analysis",
        "category": "DiffSense"
      },
      {
        "command": "diffsense.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      }
    ],
    "views": {
      "diffsense": [
        {
          "type": "webview",
          "id": "diffsense.analysisView",
          "name": "Git Impact Analysis",
          "when": "true"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "diffsense",
          "title": "DiffSense",
          "icon": "$(git-branch)"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "diffsense.refresh",
          "when": "view == diffsense.analysisView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc",
    "prepare-package": "node scripts/prepare-package.js",
    "clean-package": "node scripts/clean-package.js",
    "package": "npm run prepare-package && vsce package",
    "publish": "npm run prepare-package && vsce publish"
  },
  "dependencies": {
    "glob": "^10.3.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^5.2.2"
  }
}
