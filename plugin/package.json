{
  "name": "diffsense",
  "displayName": "DiffSense",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "diffsense.runAnalysis",
        "title": "Run Analysis",
        "category": "DiffSense"
      },
      {
        "command": "diffsense.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      }
    ],
    "views": {
      "diffsense": [
        {
          "type": "webview",
          "id": "diffsense.analysisView",
          "name": "Git Impact Analysis",
          "when": "true"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "diffsense",
          "title": "DiffSense",
          "icon": "$(git-branch)"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "diffsense.refresh",
          "when": "view == diffsense.analysisView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^5.2.2"
  }
}
